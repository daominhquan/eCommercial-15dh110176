<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme.NoActionBar" >
        <item name="colorPrimary">#131212</item>
        <item name="colorPrimaryDark">#131212</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.SplashScreen" >
        <item name="colorPrimary">#131212</item>
        <item name="colorPrimaryDark">#131212</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="NavigationView">
        <item name="android:textColorSecondary">#696969</item>
    </style>
    <style name="MyRatingBar" parent="Widget.AppCompat.RatingBar">
        <item name="colorAccent">#6200ea</item>
        <item name="android:colorControlNormal">#f9a825</item>
    </style>
